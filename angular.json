{
  "version": 1,
  "projects": {
    "ng-app-platform": {
      "projectType": "application",
      "root": "apps/ng-app-platform",
      "sourceRoot": "apps/ng-app-platform/src",
      "prefix": "ng-app-platform",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/apps/ng-app-platform",
            "index": "apps/ng-app-platform/src/index.html",
            "main": "apps/ng-app-platform/src/main.ts",
            "polyfills": "apps/ng-app-platform/src/polyfills.ts",
            "tsConfig": "apps/ng-app-platform/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ng-app-platform/src/favicon.ico",
              "apps/ng-app-platform/src/assets"
            ],
            "styles": ["apps/ng-app-platform/src/styles.scss"],
            "scripts": [],
            "extraWebpackConfig": "apps/ng-app-platform/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ng-app-platform/src/environments/environment.ts",
                  "with": "apps/ng-app-platform/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "extraWebpackConfig": "apps/ng-app-platform/webpack.prod.config.js"
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "ng-app-platform:build",
            "extraWebpackConfig": "apps/ng-app-platform/webpack.config.js",
            "port": 4200
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-app-platform:build:production",
              "extraWebpackConfig": "apps/ng-app-platform/webpack.prod.config.js"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-app-platform:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ng-app-platform/src/**/*.ts",
              "apps/ng-app-platform/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "outputs": ["coverage/apps/ng-app-platform"],
          "options": {
            "jestConfig": "apps/ng-app-platform/jest.config.js",
            "passWithNoTests": true,
            "extraWebpackConfig": "apps/ng-app-platform/webpack.config.js"
          }
        }
      }
    },
